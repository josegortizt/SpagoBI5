<module>
      <!-- Inherit the core Web Toolkit stuff.                        -->
      <inherits name='com.google.gwt.user.User'/>
      
      <!-- Other module inherits                                      -->
      
	  <!-- PALO CORE -->
      <inherits name='com.tensegrity.palo.gwt.core.Core'/>
      
	  <!-- Inherit gwt-dnd support                   -->
      <inherits name='com.allen_sauer.gwt.dnd.gwt-dnd'/>

	  <inherits name='com.google.gwt.i18n.I18N' />
	  <extend-property name="locale" values="de" />
	  <extend-property name="locale" values="en" />
 	  <extend-property name="locale" values="fr" />
 
      <entry-point class='com.tensegrity.palo.gwt.widgets.client.Widgets' />

      <!-- Specify the application specific style sheet.              -->
      <stylesheet src='palo-gwt-widgets.css' />
      <stylesheet src='styles/palotable.css' />
      <stylesheet src='styles/vieweditor.css' />
	
</module>
